{
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File",
            "Serilog.Sinks.Seq"
        ],
        "MinimumLevel": "Information",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/applog-.txt",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "",
                    "apiKey": ""
                }
            }
        ]
    },
    "OpenTelemetry": {
        "Endpoint": ""
    },
    "ConnectionStrings": {
        "DefaultConnection": "",
        "Monster": ""
    },
    "JwtOptions": {
        "SecretKey": "",
        "Issuer": "",
        "Audience": "",
        "ExpiresInMinutes": 15
    },
    "Google": {
        "ClientId": "",
        "ClientSecret": "",
        "AndroidClientId": ""
    },
    "PaymobOptions": {
        "ApiKey": "",
        "SecretKey": "",
        "PublicKey": "",
        "HMAC": "",
        "CardIntegrationId": 0,
        "WalletIntegrationId": 0,
        "DiscountImageUrl": "https://jewellery.theknight.tech/discount_image.png",
        "ShippingImageUrl": "https://jewellery.theknight.tech/shipping_image.png"
    },
    "CloudflareR2": {
        "AccountId": "",
        "BucketName": "",
        "PublicDomain": "",
        "AccessKeyId": "",
        "SecretAccessKey": ""
    },
    "Cors": {
        "DashboardOrigins": [
            "https://eshop-dashboard.theknight.tech",
            "https://eshop-dashboard.vercel.app/",
            "http://localhost:3000"
        ]
    },
    "Email": {
        "FromAddress": "noreply@eshop.com", // sender email address
        "SmtpHost": "localhost", // actual smtp host
        "SmtpPort": 25,
        "SmtpEnableSsl": false,
        "SmtpUsername": "", // email
        "SmtpPassword": "" // app password
    }
}